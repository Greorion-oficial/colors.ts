{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "no-console": [false],
        "variable-name": ["allow-leading-underscore"],
        "only-arrow-functions": ["allow-declarations"],
        "semicolon": [true, "always", "ignore-interfaces"]
    },
    "rulesDirectory": []
}
